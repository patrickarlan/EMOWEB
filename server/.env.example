DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=12345
DB_NAME=emoweb

# Optional: change the server port
PORT=4000

# For CORS in dev (Vite) set to http://localhost:5173 or leave blank for '*'
CORS_ORIGIN=

# JWT settings: change the secret in production
JWT_SECRET=please-change-this-secret
JWT_EXPIRES=7d

# Super Admin Account (not stored in database)
SUPER_ADMIN_USERNAME=superadmin
SUPER_ADMIN_EMAIL=admin@emoweb.com
SUPER_ADMIN_PASSWORD=SuperSecureAdmin123!

# Raspberry Pi Flask Server URL
# After starting the Flask server on your Pi, update this with your Pi's IP
RASPBERRY_PI_URL=http://127.0.0.1:5000
# Or use IP address: http://192.168.1.100:5000
